{
  "name": "trinity/admin",
  "description": "Trinity Admin bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Trinity Team",
      "email": "info@modpreneur.com"
    }
  ],

  "config": {
    "vendor-dir": "htdocs/vendor"
  },

  "require": {
    "php": ">=7.1",
    "symfony/symfony": "~3.3",
    "symfony/framework-bundle": "~2.8 | ~3.3",
    "symfony/form": "~2.8 | ~3.3",
    "nette/utils": "~2.3",
    "modpreneur/bowtie": "dev-master",
    "whiteoctober/breadcrumbs-bundle": "~1.2",
    "twig/twig": "~1.0|~2.0",
    "symfony/twig-bundle": "^3.3",
    "trinity/settings": "~1.2",
    "symfony/monolog-bundle": "~3.0",
    "sensio/framework-extra-bundle": "^3.0",
    "knplabs/knp-menu-bundle": "~2.0",
    "knplabs/knp-gaufrette-bundle": "~0.3",
    "symfony/assetic-bundle": "^2.8",
    "twig/extensions": "~1.5",
    "symfony/twig-bridge": "~3.3"
  },

  "require-dev": {
    "sensio/generator-bundle": "~3.0",
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~0.9",
    "phpstan/phpstan": "~0.7"
  },

  "suggest": {
    "knplabs/knp-menu": "Required to use KnpMenuBundle.",
    "modpreneur/bowtie": "Bowtie css installed."
  },


  "autoload": {
    "files": [ "htdocs/app/AppKernel.php" ],
    "psr-4": { "Trinity\\AdminBundle\\": "src/AdminBundle/" },
    "classmap": [
      "htdocs/AdminBundle/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
