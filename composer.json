{
  "name": "trinity/admin",
  "description": "Trinity Admin bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Trinity Team",
      "email": "info@modpreneur.com"
    }
  ],

  "require": {
    "php": ">=5.5",
    "symfony/framework-bundle": "~2.6",
    "symfony/twig-bundle":      "~2.6",
    "symfony/form":             "~2.6",
    "symfony/console":          "~2.6",
    "symfony/finder":           "~2.6",
    "nette/utils": "~2.3",
    "trinity/framework": "~1.3",
    "modpreneur/bowtie"  : "1.*"
  },

  "require-dev": {
    "sensio/generator-bundle": "~2.3",
    "phpunit/phpunit": "4.7.*",
    "liip/functional-test-bundle": "^1.3@dev",
    "knplabs/knp-menu":             "~2.0@alpha",
    "knplabs/knp-menu-bundle":      "~2.0@alpha",
    "knplabs/knp-paginator-bundle": "dev-master",
    "mockery/mockery":              "0.9.*"
  },


  "suggest": {
    "knplabs/knp-menu": "Required to use KnpMenuBundle.",
    "modpreneur/bowtie": "Bowtie css installed."
  },


  "autoload": {
    "classmap": [
      "AdminBundle/"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "target-dir": "AdminBundle"
}
