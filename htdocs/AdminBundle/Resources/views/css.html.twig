{% block form %}

    <form name="necktie_product" method="post" action="#" class="edit-form"
          novalidate="novalidate"><input type="hidden" name="_method" value="PUT">
        <div class="row " id="row_necktie_product_name">
            <div class="form-content-center span-none-small-18 span-medium-22 span-large-21 span-xlarge-17 margin-auto">
                <div class="span-none-padding-medium-9 span-none-padding-large-7 span-none-padding-xlarge-7 form-left ">
                    <label for="necktie_product_name" class="required">Name</label></div>
                <div class="span-none-padding-small-24 span-none-padding-medium-13 span-none-padding-large-12 span-none-padding-xlarge-11 form-right">
                    <input type="text" id="necktie_product_name" name="necktie_product[name]" required="required"
                           value="GoDaddy">
                </div>
            </div>
        </div>

        <div class="row  choice" id="row_necktie_product_clients">
            <div class="form-content-center span-none-small-18 span-medium-22 span-large-21 span-xlarge-17 margin-auto">
                <div class="span-none-padding-medium-9 span-none-padding-large-7 span-none-padding-xlarge-7 form-left ">
                    <label class="required">Projects</label></div>
                <div class="span-none-padding-small-24 span-none-padding-medium-13 span-none-padding-large-12 span-none-padding-xlarge-11 form-right">
                    <div class="widget-expanded" id="necktie_product_clients">
                        <div class="checkbox"><input type="checkbox" id="necktie_product_clients_1"
                                                     name="necktie_product[clients][]" value="1"
                                                     checked="checked"><label for="necktie_product_clients_1">ClickAndCoach</label>
                        </div>
                        <label for="necktie_product_clients_1">ClickAndCoach</label>
                        <div class="checkbox"><input type="checkbox" id="necktie_product_clients_2"
                                                     name="necktie_product[clients][]" value="2"
                                                     checked="checked"><label
                                    for="necktie_product_clients_2">HotAtHome</label>
                        </div>
                        <label for="necktie_product_clients_2">HotAtHome</label>
                        <div class="checkbox"><input type="checkbox" id="necktie_product_clients_3"
                                                     name="necktie_product[clients][]" value="3"><label
                                    for="necktie_product_clients_3">Venice</label>
                        </div>
                        <label for="necktie_product_clients_3">Venice</label></div>
                </div>
            </div>
        </div>

        <div class="row  choice" id="row_necktie_product_mainClient">
            <div class="form-content-center span-none-small-18 span-medium-22 span-large-21 span-xlarge-17 margin-auto">
                <div class="span-none-padding-medium-9 span-none-padding-large-7 span-none-padding-xlarge-7 form-left ">
                    <label for="necktie_product_mainClient">Main Project</label></div>
                <div class="span-none-padding-small-24 span-none-padding-medium-13 span-none-padding-large-12 span-none-padding-xlarge-11 form-right">
                    <div class="input-postfix-icon form-select">
                        <div class="select-style form-control">
                            <select id="necktie_product_mainClient" name="necktie_product[mainClient]"
                                    class="user-success" aria-invalid="false">
                                <option value=""></option>
                                <option value="1" selected="selected">ClickAndCoach</option>
                                <option value="2">HotAtHome</option>
                                <option value="3">Venice</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row " id="row_necktie_product_description">
            <div class="form-content-center span-none-small-18 span-medium-22 span-large-21 span-xlarge-17 margin-auto">
                <div class="span-none-padding-medium-9 span-none-padding-large-7 span-none-padding-xlarge-7 form-left ">
                    <label for="necktie_product_description">Description</label></div>
                <div class="span-none-padding-small-24 span-none-padding-medium-13 span-none-padding-large-12 span-none-padding-xlarge-11 form-right">
                    <textarea id="necktie_product_description" name="necktie_product[description]"></textarea></div>
            </div>
        </div>

        <div class="row">
            <button type="submit" id="necktie_product_submit" name="necktie_product[submit]"
                    class="button button-success">Update
            </button>

        </div>

        <input type="hidden" id="necktie_product__token" name="necktie_product[_token]"
               value="AqnfBZjF3tyQ2psHNRDatBVU_-vYF0fJNAiDw7m1gKI">
    </form>

