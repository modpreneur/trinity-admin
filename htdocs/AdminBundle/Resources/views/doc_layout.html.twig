{% extends "TrinityAdminBundle::extend_layout.html.twig" %}

    {% block navbar %}
        {{ parent() }}
        <div style="padding-left: 240px; width: 100%; z-index: 80;">
            <div data-ng-scope="queryBuilderWrapper" class="display-none padding-20" style="background: #f2f6fc"> Loading...</div>
        </div>
    {% endblock %}
    {% block navbar_logo %}
        <a class="color-white" href="/">
            <i class="necktie necktielogo3"></i>
        </a>
    {% endblock %}

    {% block title %} Trinity Administration {% endblock %}

    {% block flashMessages %}

        {{ parent() }}
    {% endblock %}

    {% block stylesheets %}
        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('favicons/apple-touch-icon.png') }}">
        <link rel="icon" type="image/png" href="{{ asset('favicons/favicon-32x32.png') }}" sizes="32x32">
        <link rel="icon" type="image/png" href="{{ asset('favicons/favicon-16x16.png') }}" sizes="16x16">
        <link rel="manifest" href="{{ asset('favicons/manifest.json') }}">
        <link rel="mask-icon" href="{{ asset('favicons/safari-pinned-tab.svg') }}" color="#5718d0">
        <link rel="shortcut icon" href="{{ asset('favicons/favicon.ico') }}?v=3">
        <meta name="msapplication-config" content="{{ asset('favicons/browserconfig.xml') }}">
        <meta name="theme-color" content="#ffffff">

        <link rel="stylesheet" href="{{ asset("css/bowtieFixes.css") }}"/>
        <link rel="stylesheet" href="{{ asset("assetic/necktie_icons.css") }}"/>

        {% if useChartJSLibraries is defined and useChartJSLibraries %}
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('css/c3.css') }}"/>
        {% endif %}
    {% endblock %}

    {% block search_action %}
        Search
    {% endblock %}


    {% block user_label %}
        <span class="user-name">
            John Dee
            <i class="tiecons tiecons-player-play-negative"></i>
        </span>

        <div class="user-photo">
            <i class="trinity trinity-profile"></i>
        </div>
    {% endblock %}


    {% block sidebar_user %}
        <ul class="quick-links">
            <li>
                <a title="{{ 'admin.layout.sidebar_user.user_profile_title'|trans }}"
                   href="#"
                >
                    <i class="tiecons tiecons-user font-14 padding-14"></i>
                    User Edit
                </a>
            </li>
        </ul>
    {% endblock %}


    {% block trinity_admin_script_footer %}

        {% block necktieJavascript %}
        {% endblock %}
        {{ parent() }}
    {% endblock %}

{% block sidebar %}
    {{ knp_menu_render('sidebar', {"currentClass": "active", "template" : "TrinityAdminBundle::kmp_menu.html.twig"}) }}
{% endblock %}
